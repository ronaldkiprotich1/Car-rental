#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/DEV/Projects/Typescript/api/node_modules/.pnpm/new-package@0.1.0/node_modules/new-package/bin/node_modules:/mnt/c/DEV/Projects/Typescript/api/node_modules/.pnpm/new-package@0.1.0/node_modules/new-package/node_modules:/mnt/c/DEV/Projects/Typescript/api/node_modules/.pnpm/new-package@0.1.0/node_modules:/mnt/c/DEV/Projects/Typescript/api/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/DEV/Projects/Typescript/api/node_modules/.pnpm/new-package@0.1.0/node_modules/new-package/bin/node_modules:/mnt/c/DEV/Projects/Typescript/api/node_modules/.pnpm/new-package@0.1.0/node_modules/new-package/node_modules:/mnt/c/DEV/Projects/Typescript/api/node_modules/.pnpm/new-package@0.1.0/node_modules:/mnt/c/DEV/Projects/Typescript/api/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../new-package/bin/new.js" "$@"
else
  exec node  "$basedir/../new-package/bin/new.js" "$@"
fi
